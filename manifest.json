{
    "manifest_version": 2,
    "name": "Random Image",
    "description": "Get Some cool random images by clicking refresh",
    "version": "1.0.0",
    "icons": {
        "128": "icon-128.png"
    },
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "permissions":["activeTab"]
}